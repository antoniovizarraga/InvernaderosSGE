<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="InvernaderoMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:InvernaderoMAUI"
    xmlns:pages="clr-namespace:InvernaderoMAUI.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="InvernaderoMAUI">

    <ShellContent
        Title="Invernaderos"
        ContentTemplate="{DataTemplate pages:ElegirInvernadero}"
        Route="ElegirInvernadero" />
    <ShellContent
        Title="Temperatura y Humedad del Invernadero"
        ContentTemplate="{DataTemplate pages:MostrarTemperatura}"
        Route="MostrarTemperatura" />

</Shell>
